class Rectangle:
    def __init__(self,leng,wid):
        self.leng=leng
        self.wid=wid
    def rectangle_area(self):
        return self.leng*self.wid
    def rectangle_peri(self):
        return 2*(self.leng+self.wid)
rectangle1=Rectangle(3,2)
rectangle2=Rectangle(4,3)
print("area of:",rectangle1.rectangle_area())
print("peri of:",rectangle1.rectangle_peri())
print("area of:",rectangle2.rectangle_area())
print("peri of:",rectangle2.rectangle_peri())
if(rectangle1.rectangle_area()==rectangle2.rectangle_area()):
    print("area are equal")
else:
    print("area not equal")




